{
	"name": "tractor",
	"displayName": "Tractor",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "treeExplorer.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/icons/light/refresh.svg",
					"dark": "resources/icons/dark/refresh.svg"
				}
			},
			{
				"command": "treeExplorer.addEntry",
				"title": "Add"
			},
			{
				"command": "treeExplorer.editEntry",
				"title": "Edit",
				"icon": {
					"light": "resources/icons/light/edit.svg",
					"dark": "resources/icons/dark/edit.svg"
				}
			},
			{
				"command": "treeExplorer.deleteEntry",
				"title": "Delete"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "treeExplorer.refreshEntry",
					"when": "view == treeExplorer",
					"group": "navigation"
				},
				{
					"command": "treeExplorer.addEntry",
					"when": "view == treeExplorer"
				}
			],
			"view/item/context": [
				{
					"command": "treeExplorer.editEntry",
					"when": "view == treeExplorer && viewItem == node",
					"group": "inline"
				},
				{
					"command": "treeExplorer.deleteEntry",
					"when": "view == treeExplorer && viewItem == node"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tractor-explorer",
					"title": "Tractor",
					"icon": "resources/icons/dark/tractor-solid.svg"
				}
			]
		},
		"views": {
			"tractor-explorer": [
				{
					"id": "treeExplorer",
					"name": "Demo"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/mkdirp": "^0.5.2",
		"jsonc-parser": "^2.1.0",
		"mkdirp": "^0.5.1",
		"react": "^16.9.0",
		"react-bulma-components": "^2.3.0",
		"rimraf": "^2.7.1"
	}
}
