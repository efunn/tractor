// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Dev",
			"type": "shell",
			"problemMatcher": "$tsc-watch",
			"command": "${workspaceRoot}/dev/bin/tractor", 
			"args": ["selfdev"],
			"isBackground": true,
			"presentation": {
				"reveal": "always",
				"clear": true
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
	]
}
